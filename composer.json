{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-redis": "*",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-openssl": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "alfredo-ramos/parsedown-extra-laravel": "^4.0",
        "atymic/twitter": "^3.1",
        "barryvdh/laravel-debugbar": "^3.2",
        "creitive/breadcrumbs": "^3.0",
        "doctrine/dbal": "^3.3.6",
        "duzun/hquery": "^3.0.3",
        "fruitcake/laravel-cors": "^3.0.0",
        "guzzlehttp/guzzle": "^7.4.2",
        "htmlmin/htmlmin": "^9.0",
        "laravel/framework": "9.5.1",
        "laravel/helpers": "^1.1",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "league/geotools": "^1.0.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^2.1",
        "spatie/laravel-feed": "^4.0",
        "stevegrunwell/time-constants": "^1.1",
        "unisharp/laravel-settings": "^2.0",
        "weidner/goutte": "dev-l9-compatibility",
        "willvincent/feeds": "^2.3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-goutte.git"
        }
    ],
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": []
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
