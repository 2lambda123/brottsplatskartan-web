{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "ext-redis": "*",
        "laravel/framework": "^8.0",
        "laravel/ui": "^3.0",
        "willvincent/feeds": "^2.0",
        "kozz/laravel-guzzle-provider": "~6.0",
        "weidner/goutte": "^1.3.1",
        "doctrine/dbal": "^2.5",
        "creitive/breadcrumbs": "^3.0",
        "thujohn/twitter": "~2.0",
        "unisharp/laravel-settings": "^2.0",
        "alfredo-ramos/parsedown-extra-laravel": "^3.0.0",
        "laravel/tinker": "^2.0",
        "htmlmin/htmlmin": "^8.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "duzun/hquery": "^1.7",
        "barryvdh/laravel-debugbar": "^3.2",
        "spatie/laravel-feed": "^4.0",
        "jcf/geocode": "~1.3",
        "league/geotools": "@stable",
        "lullabot/amp": "^2.0",
        "laravel/helpers": "^1.1",
        "fruitcake/laravel-cors": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^9.5",
        "filp/whoops": "~2.0",
        "barryvdh/laravel-ide-helper": "2.8.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": []
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
