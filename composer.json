{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "7.1",
        "laravel/framework": "5.5.*",
        "willvincent/feeds": "1.1.*",
        "kozz/laravel-guzzle-provider": "~6.0",
        "weidner/goutte": "@dev",
        "doctrine/dbal": "^2.5",
        "creitive/laravel5-breadcrumbs": "^2.0",
        "thujohn/twitter": "~2.0",
        "unisharp/laravel-settings": "^1.0",
        "laravel/scout": "^3.0",
        "alfredo-ramos/parsedown-extra-laravel": "~0.7.0",
        "laravel/tinker": "^1.0",
        "sofa/eloquence-base": "^5.5",
        "htmlmin/htmlmin": "^5.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^0.10.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "filp/whoops": "~2.0",
        "barryvdh/laravel-debugbar": "^3.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }

}
